                    <i class="nav-icon fa fa-clock-o"></i>
                    <p>
                      Schedules
                    </p>
                  <% end %>
                </li>
                <li class="nav-item">
                  <%= link_to  my_calender_path, data: { no_turbolink: true }, class: "nav-link" do %>
                    <i class="nav-icon fa fa-calendar"></i>
